{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "prettier/prettier": ["error"],
    "class-methods-use-this": "off",
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}
